# 	dest=$(sbindir) chown=amanda chmod= \
# 		foo bar \
# 	chmod=07450 \
# 		bing
# 	dest=$(libexecdir) chmod= \
# 		$(libexec_PROGRAMS)
# INSTALLPERMS_data = \
# 	dest=$(sbindir) chown=amanda chmod= \
