			echo "  ($$cmd)"; chown="$(BINARY_OWNER):$(SETUID_GROUP)";; \
		dest=*|chown=*|chmod=*) \
			echo "  ($$cmd)"; eval $$cmd;; \
		*)  $(do_file) ;; \
	    esac; \
	done

